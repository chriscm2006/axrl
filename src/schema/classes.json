{
  "@graph": [
    {
      "id": "InterfaceTest",
      "type": "Class",
      "comment": "A single test of an generic interface in a particular state.",
      "properties": [
        "result",
        "testSubject",
        "state",
        "scope",
        "timestamp",
        "environment",
        "testEngine",
        "engineOptions",
        "testRunner"
      ]
    },
    {
      "id": "web/PageTest",
      "type": "Class",
      "subClassOf": "InterfaceTest",
      "label": "PageTest (web)",
      "comment": "A single test of a web page in a particular state."
    },
    {
      "id": "ios/UIControllerTest",
      "type": "Class",
      "subClassOf": "InterfaceTest",
      "label": "UIControllerTest (iOS)",
      "comment": "A single test of a web page in a particular state."
    },
    {
      "id": "TestSubject",
      "type": "Class",
      "comment": "The some interface that tests can be performed on"
    },
    {
      "id": "web/HTMLPage",
      "type": "Class",
      "subClassOf": "TestSubject",
      "label": "HTMLPage (web)",
      "comment": "An HTML page that tests can be performed on"
    },
    {
      "id": "ios/UIViewController",
      "type": "Class",
      "subClassOf": "TestSubject",
      "label": "UIViewController (iOS)",
      "comment": "TODO"
    },
    {
      "id": "TestResult",
      "type": "Class",
      "comment": "The result of a test.",
      "properties": ["description", "summary", "helpUrl"]
    },
    {
      "id": "TestSuccess",
      "type": "Class",
      "comment": "The result of successful a test.",
      "subClassOf": "TestResult",
      "properties": ["node", "relatedNodes"]
    },
    {
      "id": "TestInapplicable",
      "type": "Class",
      "comment": "The result of an inapplicable test.",
      "subClassOf": "TestResult",
      "properties": ["node", "relatedNodes"]
    },
    {
      "id": "TestFailure",
      "type": "Class",
      "comment": "The result of failed a test.",
      "subClassOf": "TestResult",
      "properties": [
        "node",
        "relatedNodes",
        "impact",
        "remediation"
      ]
    },
    {
      "id": "TestAbort",
      "type": "Class",
      "comment": "The result of a test that was not completed.",
      "subClassOf": "TestResult",
      "properties": ["node", "relatedNodes"]
    },
    {
      "id": "ImpactType",
      "type": "Class",
      "comment": "An issue hinders the use of an interface to some degree"
    },
    {
      "id": "Critical",
      "type": "Class",
      "subClassOf": "ImpactType",
      "comment": "An issue that stops users from accessing information or functionality"
    },
    {
      "id": "Serious",
      "type": "Class",
      "subClassOf": "ImpactType",
      "comment": "An issue that can make functionality difficult to use, or content difficult to understand"
    },
    {
      "id": "Moderate",
      "type": "Class",
      "subClassOf": "ImpactType",
      "comment": "An issue that can make functionality frustrating to use"
    },
    {
      "id": "Minor",
      "type": "Class",
      "subClassOf": "ImpactType",
      "comment": "An issue that has little to no impact on users"
    },
    {
      "id": "Node",
      "type": "Class",
      "comment": "A generalised node, or component in a user interface",
      "properties": [
        "screenshot",
        "semanticRole",
        "accessibleName"
      ]
    },
    {
      "id": "web/DOMNode",
      "type": "Class",
      "subClassOf": "Node",
      "label": "DOMNode (web)",
      "comment": "A description of a node in the DOM",
